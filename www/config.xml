<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.example"
        versionCode="10" 
        version   = "1.0.0">

    <name>SCK Contacts App</name>

    <description>
        An application to add SCK contacts to your phone
    </description>

    <author href="http://www.sck-webworks.co.uk/" email="team@sck-webworks.co.uk">
        SCK Webworks
    </author>

	<preference name="permissions" value="none"/>
	<feature name="http://api.phonegap.com/1.0/contacts"/>
	<icon src="images/sck-icon.png" width="175" height="175" />
	<gap:splash src="images/sck-icon.png" />
		
</widget>